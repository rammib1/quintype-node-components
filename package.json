{
  "name": "@quintype/components",
  "version": "1.50.1",
  "description": "Components to help build Quintype Node.js apps",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quintype/quintype-node-components.git"
  },
  "keywords": [
    "quintype"
  ],
  "files": [
    "/dist"
  ],
  "author": "Quintype Developers <dev-core@quintype.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quintype/quintype-node-components/issues"
  },
  "homepage": "https://github.com/quintype/quintype-node-components#readme",
  "dependencies": {
    "atob": "^2.1.2",
    "classnames": "^2.2.6",
    "empty-web-gif": "^1.0.0",
    "get-youtube-id": "^1.0.0",
    "lodash": "^4.17.11",
    "papaparse": "^4.6.2",
    "prop-types": "^15.6.2",
    "quintype-js": "^1.1.0",
    "react": "^16.7.0",
    "react-dfp": "^0.7.6",
    "react-redux": "^6.0.0",
    "react-youtube": "^7.8.0",
    "redux": "^4.0.1",
    "wretch": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-quintype-assets": "^1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "onchange": "4.1.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.1.0",
    "standard-version": "^4.4.0"
  },
  "scripts": {
    "build:cjs": "npx rollup -c",
    "build:es": "npx babel src/ --out-dir dist/es --copy-files",
    "build": "npm run clean:dist && npm run build:cjs && npm run build:es",
    "clean:dist": "rimraf dist",
    "prepack": "npm run build",
    "prepublishOnly": "npm install && git diff --quiet && npx standard-version && git push --follow-tags origin",
    "sync-files-to": "npx onchange --verbose --wait --await-write-finish 'src/**/*' -- ./bin-dev-scripts/sync-to.sh "
  }
}
